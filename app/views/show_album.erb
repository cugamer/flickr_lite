<div class="container">
  <form method="get" action="/users/:user_id">
    <input type="submit" value="User Profile">
  </form>
  <h1><%= @album.album_name %></h1>
  <% @images.each do |x| %>
    <a href="/albums/<%= session[:user_id] %>/photos/<%= x.id %>">
      <img class="thumb_nail" src="../../<%= x.image_file %>">
    </a>

  <% end %>


</div>
